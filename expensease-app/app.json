{
    "expo": {
        "name": "Expensease",
        "slug": "expensease",
        "version": "0.0.2",
        "scheme": "expensease",
        "orientation": "portrait",
        "icon": "./assets/default/icon.png",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "deepLinking": true,
        "splash": {
            "image": "./assets/default/splash-icon.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "ios": {
            "bundleIdentifier": "com.boratechnologies.expensease",
            "googleServicesFile": "./GoogleService-Info.plist",
            "supportsTablet": true,
            "usesAppleSignIn": true,
            "icon": "./assets/default/ios/icon-ios-10241x.png",
            "associatedDomains": [
                "applinks:expensease.in"
            ],
            "infoPlist": {
                "UIBackgroundModes": [
                    "remote-notification"
                ]
            },
            "appleTeamId": "2TA4D4M36W",
            "userInterfaceStyle": "automatic"
        },
        "android": {
            "package": "com.praneelbora.expensease",
            "googleServicesFile": "./google-services.json",
            "intentFilters": [
                {
                    "action": "VIEW",
                    "data": [
                        {
                            "scheme": "https",
                            "host": "expensease.in",
                            "pathPrefix": "/"
                        }
                    ],
                    "category": [
                        "BROWSABLE",
                        "DEFAULT"
                    ]
                }
            ],
            "adaptiveIcon": {
                "foregroundImage": "./assets/default/android/adaptive-foreground.png",
                "backgroundColor": "#ffffff",
                "userInterfaceStyle": "automatic"
            }
        },
        "web": {
            "favicon": "./assets/default/favicon.png"
        },
        "plugins": [
            "expo-router",
            [
                "@react-native-google-signin/google-signin/app.plugin.js",
                {
                    "iosUrlScheme": "com.googleusercontent.apps.755754243643-s1ju6pe90tmrstpf54vl4hkik8a33ne8"
                }
            ],
            [
                "expo-notifications",
                {
                    "icon": "./assets/default/favicon.png",
                    "color": "#00C49F",
                    "defaultChannel": "default",
                    "enableBackgroundRemoteNotifications": true
                }
            ],
            "android-credential-manager",
            "expo-font",
            "expo-secure-store",
            "expo-asset",
            "expo-apple-authentication"
        ],
        "extra": {
            "router": {
                "origin": "expensease://"
            },
            "eas": {
                "projectId": "c1c6dfe4-de93-4bae-ab13-95ccf7e1d6a7"
            }
        }
    }
}